<div class="container-fluid">
  <div class="row grid-content">
    <div class="col-md-8 col-md-offset-2">
      <div class="row">
        <% 10.times do |i| %>
          <div class="col-md-2 col-sm-4 col-xs-6 cell <%= i%5 == 0 ? 'col-md-offset-1' : '' %>">
            <div class="dummy"></div>
            <div class="cell-content"></div>
          </div>
        <% end %>
      </div>
    </div>
    <div class="col-md-2"></div>
  </div>
</div>